D:\Pascal\carantine\tasks\rectangle_S\1.pasprogram write_name;
uses crt;
var file_t: text;
str: char;
begin
    clrscr;
    assign(file_t, 'text.txt');
    reset(file_t);
    while not Eof(file_t) do
    begin
        read(file_t, str);
        write(str);
    end;
    close(file_t);
    readln;
end.
